<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th>DEPARTURE</th>
      <th>ARRIVAL</th>
      <th>DETAILS</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(function(m) { %>
      <tr>
        <td><%= m.departure %></td>
        <td><%= m.arrival %></td>
        <td><a href="/flights/<%= m._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
